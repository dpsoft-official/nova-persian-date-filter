{
    "name": "dpsoft/nova-persian-date-filter",
    "description": "A Laravel Nova filter.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Dpsoft\\NovaPersiandateFilter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dpsoft\\NovaPersiandateFilter\\FilterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
